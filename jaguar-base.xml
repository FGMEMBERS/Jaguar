      </generic>
    </multiplay>

    <systems>
      <electrical>
        <path>Aircraft/Generic/generic-electrical.xml</path>
      </electrical>

      <autopilot>
        <path>Aircraft/Jaguar/System/jag-autopilot.xml</path>
      </autopilot>

      <refuel>
        <type>probe</type>
        <serviceable type="bool">true</serviceable>
      </refuel>
    </systems> 

