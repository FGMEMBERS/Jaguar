<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <key n="101">
    <name>e</name>
    <desc>Guns</desc>
    <binding>
      <command>nasal</command>
      <script>guns.fire_MG()</script>
    </binding>
    <mod-up>
      <binding>
        <command>nasal</command>
        <script>guns.stop_MG()</script>
      </binding>
    </mod-up>
  </key>

